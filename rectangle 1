//to find the area of rectangle 


#include <stdio.h>

int main(void)
{
    int length , breadth ;
    
    //to input length
    printf("enter length : ");
    scanf("%d",&length);

   
    //to input breadth
    printf("enter breadth : ");
    scanf("%d",&breadth);

    //to get the area of rectangle
    printf("the area of rectangle is %d\n", length * breadth);


}
